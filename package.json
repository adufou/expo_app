{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@tamagui/core": "2.0.0-rc.4",
    "@tamagui/metro-plugin": "2.0.0-rc.4",
    "@tamagui/switch": "2.0.0-rc.4",
    "expo": "~54.0.33",
    "expo-status-bar": "~3.0.9",
    "i18next": "^25.8.4",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-i18next": "^16.5.4",
    "react-native": "0.83.1",
    "react-native-web": "^0.21.0",
    "tamagui": "2.0.0-rc.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tamagui/babel-plugin": "2.0.0-rc.4",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.2.13",
    "@vitest/eslint-plugin": "^1.6.6",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "private": true
}
